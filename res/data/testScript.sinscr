set testBool false;
set testNum 204;
set testNumCheck 205;
set outStr "Yay! testNum ($testNum$) is greater than $testNumCheck$!$nThis is the best day of my life!";
set outStr1 "testBool is now $testBool$!";

FUNC checkTestNum();
	IF testNum gtrEql testNumCheck THEN;
		echo outStr;
		set testBool true;
	ENDIF;
ENDFUNC;

FUNC print(str s);
	IF s notEql nil THEN;
		echo s;
	ENDIF;
ENDFUNC;

inc testNum;
checkTestNum();
print(outStr1);

END